/* dash board vertical scroll for IE only */
div.tableContainerIE {
   position: relative;
   overflow: auto;
   border: 1px solid gray;
}
div.tableContainerIE thead tr {
  position: relative;
  top: expression(this.offsetParent.scrollTop);
  background-color: #ffffff;
}
div.tableContainerIE tbody {
  height: auto;
}

div.tableContainerIE tbody td{
   border-bottom: 1px solid #E8E8E8;
   border-right: 1px solid #E8E8E8;
}
div.tableContainerIE thead th{
   border-right: 1px solid #F8F8F8;
}

